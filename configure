if test "$sim_ac_threads_api" != "none"; then
  # Check whether --enable-threadsafe or --disable-threadsafe was given.
if test "${enable_threadsafe+set}" = set; then
  enableval="$enable_threadsafe"

    case $enableval in
      yes) sim_ac_enable_threadsafe=true ;;
      *) ;;
    esac
fi;
fi

sim_ac_coin_threadsafe=false
if $sim_ac_enable_threadsafe; then
  sim_ac_coin_threadsafe=true

cat >>confdefs.h <<\EOF
#define COIN_THREADSAFE 1
EOF

  if test x${sim_ac_configuration_settings+set} != xset; then
  sim_ac_configuration_settings="Thread safe:Yes  (in development)"
else
  sim_ac_configuration_settings="$sim_ac_configuration_settings|Thread safe:Yes  (in development)"
fi

else
  if test x${sim_ac_configuration_settings+set} != xset; then
  sim_ac_configuration_settings="Thread safe:No  (enable with --enable-threadsafe)"
else
  sim_ac_configuration_settings="$sim_ac_configuration_settings|Thread safe:No  (enable with --enable-threadsafe)"
fi

fi

