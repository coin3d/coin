  enableval="$enable_compact"
  case "${enableval}" in
    yes) enable_compact=yes ;;
    no)  enable_compact=no ;;
    *) { echo "configure: error: bad value \"${enableval}\" for --enable-compact" 1>&2; exit 1; } ;;
  esac
else
  enable_compact=no
fi




