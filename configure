        # CFLAGS="-g $CFLAGS"
        # CXXFLAGS="-g $CXXFLAGS"
        LIBFLAGS="$LIBFLAGS /DEBUG /PDB:coin$COIN_MAJOR_VERSION$SUFFIX.pdb"
        ;;
      *)

if test x"${sim_ac_configuration_settings+set}" = x"set"; then
  sim_ac_configuration_settings="$sim_ac_configuration_settings|Coin build type:dynamic .dll + .pdb"
else
  sim_ac_configuration_settings="Coin build type:dynamic .dll + .pdb"
