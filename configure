        CFLAGS="-g $CFLAGS"
        CXXFLAGS="-g $CXXFLAGS"
        ;;
      esac
    else
      LIBFLAGS="-DLL /INCREMENTAL:NO $LIBFLAGS"
      case $enable_symbols in
      no | false)

